<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.samuniz.billspaid_android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_atalho"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_atalho_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.CadastroActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.ContasActivity" />
        <activity android:name=".Activities.DespesasActivity" />
        <activity android:name=".Activities.ReceitasActivity"></activity>
    </application>

</manifest>